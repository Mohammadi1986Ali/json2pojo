{
  "$schema": "http://tutorial.ai4everyone.com",
  "type": "object",
  "properties": {
    "post_office": {
      "type": "string"
    },
    "street_address": {
      "type": "string"
    },
    "locality": {
      "type": "string"
    },
    "postal_code": {
      "type": "string"
    },
    "country_name": {
      "type": "string"
    }
  },
  "required": [ "locality", "region", "country-name" ],
  "dependencies": {
    "post-office-box": [ "street-address" ]
  }
}